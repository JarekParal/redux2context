{
  "name": "reactizer",
  "version": "0.0.0",
  "description": "The ultimate react boilerplate!",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run clean && npm run babel && run-p server webpack:dev",
    "server": "node dist/server",
    "render": "node dist/server/render",
    "bundle": "npm run clean && npm run babel && npm run webpack && npm run render",
    "babel": "babel src --out-dir dist",
    "webpack": "webpack --config etc/webpack.build.js",
    "webpack:dev": "webpack-dev-server --config etc/webpack.dev.js --inline --hot",
    "test": "jest",
    "lint": "eslint '**/*.{js,jsx}'",
    "lint:fix": "eslint '**/*.{js,jsx}' --fix",
    "flow": "flow",
    "flow:check": "flow check",
    "clean": "rimraf .tmp dist"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate"
  ],
  "author": "oreqizer",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.4.1",
    "koa-logger": "^3.1.0",
    "koa-static": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.5"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.64.0",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./etc/enzymeSetup"
    ]
  }
}
